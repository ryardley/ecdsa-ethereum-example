{
  "compilerOptions": {
    "lib": ["ESNext", "dom"],
    "target": "ES2022",
    "module": "nodenext",
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts"]
}
